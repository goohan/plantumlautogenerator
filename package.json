{
  "name": "plantumlautogenerator",
  "displayName": "PlantUML auto generator",
  "description": "PlantUML automatic image generator",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {"type": "git", "url": "https://github.com/goohan/plantumlautogenerator"},
  "icon": "assets/icon.png",
  "license": "MIT",
  "publisher": "goohan",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "plantumlautogenerator.generatediagramimage",
        "title": "Generate diagram image"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "plantumlautogenerator.generatediagramimage",
          "when": "resourceLangId == plantuml"
        }
      ]
    },
    "languages": [
      {
        "id": "plantuml",
        "extensions": [
          ".wsd",
          ".pu",
          ".puml",
          ".plantuml",
          ".iuml"
        ],
        "aliases": [
          "PlantUML"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "node-plantuml": "^0.9.0"
  }
}
